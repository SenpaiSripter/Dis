-- Scripts:

local function ECRL_fake_script() -- Minimize.LocalScript 
	local script = Instance.new('LocalScript', Minimize)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.openframe.Visible = true
	end)
end
coroutine.wrap(ECRL_fake_script)()
local function RHZIPWM_fake_script() -- TextButton_2.LocalScript 
	local script = Instance.new('LocalScript', TextButton_2)

	script.Parent.MouseButton1Down:Connect(function()
		script.Parent.Parent.Visible = false
		script.Parent.Parent.Parent.frame.Visible = true
	end)
end
coroutine.wrap(RHZIPWM_fake_script)()
